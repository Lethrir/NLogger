<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="nLogger" type="NLogger.NLoggerSection, NLogger" allowDefinition="Everywhere" allowLocation="true" />
  </configSections>
  <appSettings>
    <add key="LogFile" value="C:\Logs\Log.log"/>
  </appSettings>
  <nLogger filePath="C:\Logs\Log.log" fileSize="10" numFiles="3" logLevel="Error">
    
  </nLogger>
</configuration>